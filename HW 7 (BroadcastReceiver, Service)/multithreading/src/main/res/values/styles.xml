<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="homeAsUpIndicator">@drawable/home_add_contact</item>

    </style>

    <style name="MultithreadingRegimeActivity_titleTextStyle">
        <item name="android:textColor">@color/multithreadingRegimeActivity_titleTextColor</item>
        <item name="android:textSize">@dimen/multithreadingRegimeActivity_titleTextSize</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="MultithreadingRegimeActivity_generalTextStyle">
        <item name="android:textColor">@color/multithreadingRegimeActivity_generalTextColor</item>
        <item name="android:textSize">@dimen/multithreadingRegimeActivity_generalTextSize</item>
        <item name="android:gravity">left|center_vertical</item>
    </style>

</resources>